<div id="star-comf<%= book.id %>"></div>
  <script>
    let raty_<%= book.id %> = document.querySelector('#star-comf<%= book.id %>');
    let option_<%= book.id %> = {
    starOff:  "<%= asset_path('star-off.png') %>",
    starOn : "<%= asset_path('star-on.png') %>",
    starHalf: "<%= asset_path('star-half.png') %>",
    readOnly: true,
    score: "<%= book.score %>",
    };
    raty(raty_<%= book.id %>,option_<%= book.id %>);
  </script>